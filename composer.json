{
    "name": "msx/portal",
    "description": "Pacote simples para listagem dos dados de um portal",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Msx\\Portal\\": "src/"
        }
    },
    "authors": [
        {
            "name": "MSX Contents",
            "email": "suporte@msxcontents.com"
        }
    ],
    "require-dev": {
        "pestphp/pest": "^3.5"
    },
    "require": {
        "php": ">=7.4",
        "ext-pdo": "*",
        "ext-json": "*",
        "smarty/smarty": "^5.4"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
